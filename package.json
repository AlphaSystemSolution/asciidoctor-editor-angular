{
  "name": "asciidoc-editor-angular2",
  "version": "1.0.0",
  "description": "Angular 2 based AsciiDoc Editor",
  "main": "gulpfile.js",
  "private": true,
  "scripts": {
    "develop": "concurrently \"tsc -w -p ./server\" \"tsc -w -p ./client\" \"nodemon ./build/server/bin/www\" ",
    "twc:client:w": "tsc -w -p ./client",
    "compile-client": "tsc -p ./client",
    "twc:server:w": "tsc -w -p ./server",
    "compile-server": "tsc -p ./server",
    "typings": "typings install",
    "bundle": "node tools/builder.js",
    "bundle:prod": "node tools/builder.js --prod",
    "postinstall": "typings install && tsc -p ./server"
  },
  "engines": {
    "node": ">= 5.4.1 < 6"
  },
  "author": "Syed Farhan Ali",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.0.1",
    "@angular/compiler": "^2.0.1",
    "@angular/core": "^2.0.1",
    "@angular/forms": "^2.0.1",
    "@angular/http": "^2.0.1",
    "@angular/platform-browser": "^2.0.1",
    "@angular/platform-browser-dynamic": "^2.0.1",
    "@angular/router": "^3.0.1",
    "@angular/upgrade": "^2.0.1",
    "angular2-in-memory-web-api": "0.0.21",
    "angular2-jwt": "^0.1.23",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "jsonwebtoken": "^7.1.9",
    "monk": "^3.1.2",
    "nodemon": "^1.10.2",
    "primeng": "^1.0.0-beta.17",
    "primeui": "^4.1.15",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-beta.12",
    "serve-favicon": "^2.3.0",
    "systemjs": "^0.19.39",
    "systemjs-builder": "^0.15.32",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "yargs": "^6.0.0"
  }
}
